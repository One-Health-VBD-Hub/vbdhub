{
  "name": "@vbdhub/service",
  "version": "0.1.0",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sync:gbif": "node dist/jobs/sync gbif 15",
    "sync:vt": "node dist/jobs/sync vt 10",
    "sync:vd": "node dist/jobs/sync vd 10",
    "sync:hub": "node dist/jobs/sync hub 10",
    "sync:px": "node dist/jobs/sync px 15",
    "knip": "knip",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org imperial-college-london-ls --project hub-backend ./dist && sentry-cli sourcemaps upload --org imperial-college-london-ls --project hub-backend ./dist"
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "none"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9",
    "@fastify/helmet": "^13",
    "@fastify/static": "^8.2",
    "@nestjs/axios": "^4",
    "@nestjs/cache-manager": "^3",
    "@nestjs/common": "^11",
    "@nestjs/config": "^4",
    "@nestjs/core": "^11",
    "@nestjs/platform-fastify": "^11",
    "@nestjs/swagger": "^11.2.0",
    "@sentry/cli": "^2",
    "@sentry/nestjs": "^10",
    "@sentry/profiling-node": "^10",
    "@turf/buffer": "^7.2.0",
    "@turf/kinks": "^7.2.0",
    "@turf/rewind": "^7.2.0",
    "@turf/simplify": "^7.2.0",
    "axios": "^1",
    "axios-retry": "^4",
    "cache-manager": "^6",
    "class-transformer": "^0.5",
    "class-validator": "^0.14",
    "globals": "^16",
    "p-limit": "^6",
    "pidusage": "^4",
    "reflect-metadata": "^0.2",
    "rxjs": "^7",
    "string-strip-html": "^13",
    "wellknown": "^0.5",
    "xml-class-transformer": "^3",
    "minio": "^8",
    "csv-parse": "^6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9",
    "@nestjs/cli": "^11",
    "@nestjs/schematics": "^11",
    "@nestjs/testing": "^11",
    "@swc/cli": "^0.6",
    "@swc/core": "^1",
    "@types/geojson": "^7946",
    "@types/jest": "^29",
    "@types/node": "^22",
    "@types/pidusage": "^2",
    "@types/supertest": "^6",
    "@types/wellknown": "^0.5",
    "@types/yargs": "^17",
    "eslint": "^9",
    "eslint-config-prettier": "^10",
    "eslint-plugin-prettier": "^5",
    "jest": "^29",
    "knip": "latest",
    "prettier": "^3",
    "source-map-support": "^0.5",
    "ts-jest": "^29",
    "ts-loader": "^9",
    "ts-node": "^10",
    "tsconfig-paths": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
